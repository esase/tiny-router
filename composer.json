{
  "name": "esase/tiny-router",
  "type": "library",
  "description": "Flexible routing system component for HTTP and console applications",
  "license": "MIT",
  "keywords": [
    "esase",
    "tinyframework",
    "microservices",
    "framework",
    "lightweight",
    "mvc",
    "routing"
  ],
  "support": {
  },
   "authors": [
        {
            "name": "Alex Ermashev",
            "email": "alexermashev@gmail.com"
        }
    ],
  "require": {
    "php": "^7.4 || ^8.0",
    "esase/tiny-http": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0 || ^10.0",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Tiny\\Router\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TinyTest\\Router\\": "test/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
